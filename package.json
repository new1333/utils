  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "bumpp": "^10.3.1",
    "tsx": "^4.20.6",
    "unbuild": "^3.6.1",
    "vite": "^7.1.10",

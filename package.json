{
  "name": "@new1333/utils",
  "version": "0.1.2",
  "description": "Opinionated collection of common JavaScript / TypeScript utils by @new1333",
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "dist/index.mjs",
